{
  "name": "mongonna",
  "description": "When You're Gonna use HTTPS for MongoDB",
  "version": "0.0.1",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "example": "ts-node-esm ./example.ts",
    "syncpack": "syncpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts-node": "ts-node-esm",
    "rel": "release-it"
  },
  "tsup": {
    "entry": {
      "index": "index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": false
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "engines": {
    "node": ">=14.19.0"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "rfdc": "^1.3.0",
    "serialize-error": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "micro": "^9.4.1",
    "mongodb": "^4.11.0",
    "release-it": "^15.5.0",
    "syncpack": "^8.2.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "mongodb",
    "rest"
  ]
}
